{
    "manifest_version": 2,
    "name": "Site Limits",
    "description": "",
    "version": "0.0.1",
    "developer": {
        "name": "Chris Wheeler"
    },
    "browser_specific_settings": {
        "gecko": {}
    },
    "permissions": [
        "activeTab",
        "storage",
        "notifications"
    ],
    "optional_permissions": [],
    "options_page": "settings.html",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "images/drum-16.png",
        "32": "images/drum-32.png",
        "48": "images/drum-48.png",
        "96": "images/drum-96.png",
        "128": "images/drum-128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "images/drum-16.png",
            "32": "images/drum-32.png",
            "48": "images/drum-48.png",
            "96": "images/drum-96.png",
            "128": "images/drum-128.png"
        },
        "default_title": "Site Limits",
        "default_popup": "popup.html"
    },
    "commands": {}
}
